-- ----------------------------
-- Table structure for sys_role
-- ----------------------------
DROP TABLE IF EXISTS `sys_role`;
CREATE TABLE `sys_role` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `app_id` int(11) NOT NULL COMMENT '应用ID',
  `name` varchar(50) COLLATE utf8mb4_unicode_ci NOT NULL COMMENT '名称',
  `grade_type` int(11) NOT NULL DEFAULT '0' COMMENT '管理员类型\n\n如果再有按业务需求分的 管理员类型  在这个字段中添加\n\n0:通用角色\n1:校级\n2:学院级别\n3:部门级别\n\n\n',
  `grade_id` int(11) NOT NULL DEFAULT '0' COMMENT '等级 id\n\n\n根据type  \n分别去不同表中找\n\n如\n学院级别   就去DIC_COLLEGE表中找对应id\n部门级别    DIC_DEP',
  `is_grade` tinyint(1) NOT NULL DEFAULT '0' COMMENT '是否为 \n\n等级管理员\n\n如果是等级管理员\n\n与 grade_name   字段对应\n\n0:否\n1:是',
  `sort` int(11) NOT NULL COMMENT '排序',
  `description` varchar(200) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '描述',
  `is_enable` tinyint(1) NOT NULL DEFAULT '1' COMMENT '是否启用\n\n0:不可用\n1:可用',
  `gmt_create` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `gmt_modified` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `is_delete` tinyint(1) NOT NULL DEFAULT '0' COMMENT '是否删除\n\n0: 未删除\n1: 已删除',
  `type` int(11) NOT NULL DEFAULT '0',
  PRIMARY KEY (`id`),
  UNIQUE KEY `id_UNIQUE` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=4 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

