-- ----------------------------
-- Table structure for bus_agreement_audit
-- ----------------------------
DROP TABLE IF EXISTS `bus_agreement_audit`;
CREATE TABLE `bus_agreement_audit` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '合同审批表',
  `lawyer_id` int(11) NOT NULL COMMENT '律师id',
  `agreement_id` int(11) NOT NULL COMMENT '合同id',
  `first_agreement_name` varchar(300) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '初审合同',
  `second_agreement_name` varchar(300) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '终审合同',
  `score` double NOT NULL DEFAULT '0' COMMENT '评分',
  `type` int(11) NOT NULL DEFAULT '0' COMMENT '合同类型\n0：-\n1：买卖合同\n2：供水电气热力合同\n3：赠与合同\n4：借款合同\n5：租赁合同\n6：股权合同\n7：融资租赁合同\n8：承揽合同\n9：建设工程合同\n10：物流运输合同\n11：技术合同\n12：保管合同\n13：仓储合同\n14：委托合同\n15：行纪合同\n16：居间合同\n17：混合法律关系合同\n18：其他合同',
  `gmt_create` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `gmt_modified` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `is_delete` tinyint(1) NOT NULL DEFAULT '0' COMMENT '是否删除\n\n0: 未删除\n1: 已删除',
  `end_lawyer_id` int(11) NOT NULL DEFAULT '0' COMMENT '复审律师id',
  `first_audit_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT '初审回复日期',
  `state` int(11) NOT NULL DEFAULT '0' COMMENT '转移状态\n0：-\n1：未接收\n2：已接收',
  `firm_id` int(11) NOT NULL DEFAULT '0',
  `content` varchar(300) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '回复合同备注',
  PRIMARY KEY (`id`),
  UNIQUE KEY `id_UNIQUE` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

