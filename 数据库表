DROP TABLE IF EXISTS `bus_agreement`;
CREATE TABLE `bus_agreement` (
  `id` int(11) NOT NULL AUTO_INCREMENT COMMENT '合同信息',
  `name` varchar(300) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '合同名称',
  `custom_id` int(11) NOT NULL DEFAULT '0' COMMENT '客户id',
  `state` int(11) NOT NULL DEFAULT '0' COMMENT '合同状态\n0:-\n1:发布\n2:初审\n3:终审\n4:审核完成\n5:转移中',
  `end_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '截止日期',
  `audit` int(11) NOT NULL DEFAULT '0' COMMENT '需审核方\\n0：-\\n1：甲方\\n2：乙方 3丙方 4其他',
  `audit_str` varchar(45) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '审核方为其他的时候为必填',
  `remark` varchar(300) CHARACTER SET utf8mb4 NOT NULL DEFAULT '' COMMENT '备注',
  `gmt_create` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '创建时间',
  `gmt_modified` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP,
  `is_delete` tinyint(1) NOT NULL DEFAULT '0' COMMENT '是否删除\n\n0: 未删除\n1: 已删除',
  `firm_id` int(11) NOT NULL DEFAULT '0',
  `meet_name` varchar(45) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '对接人姓名',
  `meet_tel` varchar(45) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '对接人联系方式',
  `type` varchar(45) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '合同类型名称',
  `reply_mode` varchar(45) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '2' COMMENT '回复方式 1：邮箱 2：系统回复',
  `reply_mode_str` varchar(200) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '邮箱',
  `emergency_state` int(11) NOT NULL DEFAULT '1' COMMENT '紧急情况 1正常  2加急 3紧急  4特急',
  `work_nature` varchar(45) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '' COMMENT '工作性质',
  `work_state` int(11) NOT NULL DEFAULT '0' COMMENT '工作阶段',
  `begin_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '开始时间',
  `stop_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT '结束时间',
  PRIMARY KEY (`id`),
  UNIQUE KEY `id_UNIQUE` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

